# Clave API de Groq (incluye acceso a modelos de DeepSeek, Meta, etc.)
GROQ_API_KEY=
GROQ_BASE_URL=https://api.groq.com/openai/v1/chat/completions
GROQ_MODEL=compound-beta

# Configuración del servidor
MAX_PROMPT_LEN=1000
ALLOWED_ORIGINS=http://localhost

# Seguridad - API Keys para autenticación
API_KEYS=secret123,anotherKey

# Controles de robustez avanzada
TOKENS_DAILY_LIMIT=200000

# Observabilidad y alertas
PAGERDUTY_WEBHOOK=https://events.pagerduty.com/integration/your-integration-key/enqueue

# React Frontend Configuration
VITE_API_URL=http://localhost:8000
VITE_DEV_MODE=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_ANALYTICS=true

# Analytics (optional)
VITE_ANALYTICS_ID=