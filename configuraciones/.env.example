# Configuración de modelos AI
# Groq (incluye acceso a modelos de DeepSeek, Meta, etc.)
GROQ_API_KEY=
GROQ_BASE_URL=https://api.groq.com/openai/v1/chat/completions
GROQ_MODEL=deepseek-r1-distill-llama-70b

# Configuración de modelo por defecto
DEFAULT_MODEL_PROVIDER=groq

# Configuración de encriptación de claves
MASTER_PASSWORD=your_secure_admin_password_2024
USE_ENCRYPTED_KEYS=false

# Claves encriptadas (usar cuando USE_ENCRYPTED_KEYS=true)
# GROQ_API_KEY_ENCRYPTED=
# OPENAI_API_KEY_ENCRYPTED=
# SEARCH_API_KEY_ENCRYPTED=

# Configuración del servidor
MAX_PROMPT_LEN=1000
ALLOWED_ORIGINS=http://localhost

# Seguridad - API Keys para autenticación
API_KEYS=secret123,anotherKey

# Configuración de búsqueda web
SEARCH_API_KEY=
SEARCH_ENDPOINT=https://api.bing.microsoft.com/v7.0/search
WEB_SCRAPE_TIMEOUT=10
MAX_SEARCH_RESULTS=3
MAX_PAGE_LENGTH=1000
MAX_SEARCH_ITERATIONS=2

# Controles de robustez avanzada
TOKENS_DAILY_LIMIT=200000

# Observabilidad y alertas
PAGERDUTY_WEBHOOK=https://events.pagerduty.com/integration/your-integration-key/enqueue

# React Frontend Configuration
VITE_API_URL=http://localhost:8000
VITE_DEV_MODE=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_ANALYTICS=true

# Analytics (optional)
VITE_ANALYTICS_ID=